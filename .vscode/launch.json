{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/packages/vs-code"
      ],
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ],
      "cwd": "${workspaceFolder}/test",    // Set working directory here
    }
  ]
}
